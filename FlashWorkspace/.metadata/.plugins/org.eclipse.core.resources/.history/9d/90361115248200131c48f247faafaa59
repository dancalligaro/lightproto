
<script language="JavaScript"> 
    // adds two numbers, and sends the result back to ActionScript 
    function addNumbers(num1, num2) 
    { 
        return (num1 + num2); 
    } 

    
window.flashWebcam = {
	statusChanged: function(statusCode){
		if(statusCode=="Camera.Unmuted"){
			alert('Ok!')
		}else{
			alert('This will not work')
		}
	}
}
    
</script>


